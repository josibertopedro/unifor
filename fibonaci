import java.util.Scanner;

public class ex15 {
    public static void main (String[]args){
            Scanner x = new Scanner(System.in);
            System.out.println("Digite a quantidade de termos da serie Fibonacci: ");
            int numero = x.nextInt();
            int a = 1;
            int b = 0;
            int aux;
            for (int i = 0; i < numero; i++) {
                System.out.println(b);
                aux = a;
                a = a + b;
                b = aux;
            }
        }
    }


//15) Escreva um programa que leia a quantidade de termos a serem exibidas da sÃ©rie de Fibbonacci e
//mostre os respectivos termos
