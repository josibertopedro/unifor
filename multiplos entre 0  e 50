import java.util.Scanner;

public class ex12 {
    public static void main(String[] args) {
        Scanner x = new Scanner(System.in);
        double multiplos;
        System.out.println("digite quais multiplos voce quer ate 50: ");
        multiplos = x.nextDouble();
        String multipliesOf3 = "";
        for (int i = 0; i < 50; i++) {
            if (i % multiplos == 0) {
                multipliesOf3 += i + " ";
            }
        }
        System.out.println(multipliesOf3);
    }
}
//os mÃºltiplos de 3 entre 0 e 50
